{
  "name": "weaviate-client",
  "version": "3.0.0-beta.23",
  "description": "JS/TS client for Weaviate",
  "main": "dist/node/cjs/index.js",
  "type": "module",
  "exports": {
    ".": {
      "types": {
        "require": "./dist/node/cjs/index.d.ts",
        "default": "./dist/node/esm/index.d.ts"
      },
      "default": {
        "require": "./dist/node/cjs/index.js",
        "default": "./dist/node/esm/index.js"
      }
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "test": "jest --no-cache --useStderr --runInBand --detectOpenHandles",
    "test:coverage": "npm run test -- --coverage",
    "build": "npm run build:node",
    "build:web": "tsup",
    "build:cjs": "tsc --module commonjs --outDir dist/node/cjs && touch dist/node/cjs/package.json && echo '{\"type\": \"commonjs\"}' > dist/node/cjs/package.json",
    "build:esm": "tsc --module NodeNext --outDir dist/node/esm && touch dist/node/esm/package.json && echo '{\"type\": \"module\"}' > dist/node/esm/package.json",
    "build:node": "npm run lint && npm run build:esm && npm run build:esm && prettier --write --no-error-on-unmatched-pattern '**/dist/**/*.{ts,js}'",
    "prepack": "npm run build",
    "lint": "eslint --ext .ts,.js .",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write --no-error-on-unmatched-pattern '**/*.{ts,js}' '!dist/**'",
    "format:check": "prettier --check --no-error-on-unmatched-pattern '**/*.{ts,js}' '!dist/**'",
    "format:dist": "prettier --write --no-error-on-unmatched-pattern '**/dist/**/*.{ts,js}'",
    "schema": "./tools/refresh_schema.sh",
    "protos": "./tools/refresh_protos.sh",
    "docs": "typedoc --out docs/ src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weaviate/typescript-client.git"
  },
  "keywords": [
    "weaviate"
  ],
  "author": "Weaviate",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/weaviate/typescript-client/issues"
  },
  "homepage": "https://github.com/weaviate/typescript-client#readme",
  "dependencies": {
    "graphql-request": "^6.1.0",
    "graphql": "^16.8.1",
    "long": "^5.2.3",
    "nice-grpc": "^2.1.8",
    "nice-grpc-common": "2.0.2",
    "nice-grpc-client-middleware-deadline": "^2.0.11",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-typescript": "^7.24.1",
    "@babel/runtime": "^7.24.4",
    "@curveball/bodyparser": "1.0.0",
    "@curveball/core": "1.0.0",
    "@curveball/kernel": "1.0.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@types/isomorphic-fetch": "^0.0.39",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin": "1.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "graphql-request": "^6.1.0",
    "grpc-tools": "^1.12.4",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "long": "^5.2.3",
    "nice-grpc": "^2.1.8",
    "openapi-typescript": "^6.7.5",
    "prettier": "^3.2.5",
    "protobufjs": "^7.2.6",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-proto": "^1.172.0",
    "tsup": "^8.0.2",
    "typedoc": "^0.25.13",
    "typescript": "5.4.5",
    "uuid": "^9.0.1"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "npm run format:check",
      "npm run lint -- --cache"
    ]
  }
}
